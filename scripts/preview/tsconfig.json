{
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2019", "ESNext"],
    "allowJs": true,
    "checkJs": false,
    "incremental": true,
    "isolatedModules": true,
    "noEmit": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true
  }
}
